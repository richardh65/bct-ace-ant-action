                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Look for inventory between the <code>ReqStartDate</code> and the <code>ReqEndDate</code>.
					<br/>Default: Current date
					<Validation>
                                    <Condition>Must be a valid date.
						</Condition>

                                    <Error
                                    Description="Invalid Date Format." ErrorCode="YFC004"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReqStartDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Look for inventory between the <code>ReqStartDate</code> and the <code>ReqEndDate</code>.
					<br/>Default: Current date
					<Validation>
                                    <Condition>Must be a valid date.
						</Condition>

                                    <Error
                                    Description="Invalid Date Format." ErrorCode="YFC004"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
