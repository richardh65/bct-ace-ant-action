                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RegionServiced" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
