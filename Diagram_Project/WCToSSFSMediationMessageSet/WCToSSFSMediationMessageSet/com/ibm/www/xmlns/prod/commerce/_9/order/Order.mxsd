					<xsd:documentation>The order item status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="InventoryStatus" type="_ord:InventoryStatusType">
				<xsd:annotation>
					<xsd:documentation>The inventory status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="FulfillmentStatus" type="_ord:FulfillmentStatusType">
				<xsd:annotation>
					<xsd:documentation>The fufillment status.</xsd:documentation>
