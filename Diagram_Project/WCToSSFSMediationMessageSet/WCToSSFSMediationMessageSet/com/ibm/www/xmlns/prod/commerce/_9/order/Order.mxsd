			<xsd:element maxOccurs="1" minOccurs="0" name="trackingURL" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" ref="_wcf:UserData">
				<xsd:annotation>
					<xsd:documentation>The user data area.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="OrderItemStatus" type="_ord:OrderItemStatusType">
		<xsd:annotation>
			<xsd:documentation>Element definition of the order item's status.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
