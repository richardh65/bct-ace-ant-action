		<xsd:annotation>
			<xsd:documentation>Type definition of the order item's status.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="0" name="Status" type="_ord:OrderItemLifeCycleStatusType">
